{
  "name": "wljs-editor",
  "version": "0.2.8",
  "description": "WLJS Code editor",
  "scripts": {
    "build": "node --max-old-space-size=8192 ./node_modules/.bin/rollup --config rollup.config.mjs",
    "restore": "mkdir _temp && cd _temp && git clone https://github.com/JerryI/wljs-editor && mv wljs-editor/.git ../.git && cd .. && rm -rf _temp"
  },
  "defaultPackage": true,
  "wljs-meta": {
    "jsmodule": [
      "dist/kernel.js",
      "src/boxes.js"
    ],
    "wlkernel": "src/boxes.wl",
    "wl": "src/processor.wl",
    "styles": "src/styles.css",
    "autocomplete": "src/autocomplete.js",
    "header": "src/header.wsp",
    "priority": -10,
    "important": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JerryI/wljs-editor"
  },
  "dependencies": {
    "@codemirror/lang-css": "^6.2.1",
    "@codemirror/lang-html": "6.4.2",
    "@codemirror/lang-javascript": "6.1.4",
    "@codemirror/lang-markdown": "6.1.1",
    "@codemirror/legacy-modes": "6.3.2",
    "@codemirror/state": "6.2.0",
    "@codemirror/stream-parser": "0.19.9",
    "@codemirror/view": "6.9.6",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "codemirror": "6.0.1",
    "crypto-js": "^4.1.1",
    "mma-uncompress": "^1.0.1",
    "node-balanced": "^0.0.14",
    "parcel-bundler": "1.12.5",
    "priceless-mathematica": "^1.2.1",
    "rainbowbrackets": "^2.0.2",
    "rollup": "^3.21.6"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  }
}
